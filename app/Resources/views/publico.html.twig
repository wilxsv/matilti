<!DOCTYPE html>
<html lang="es">
<head>
  <title>{% block title %}Mensajeria{% endblock %}</title>
  <meta charset="utf-8">
  <link rel="shortcut icon" href="{{ asset('bundles/matiltimensajeria/images/favicon.ico') }}" />
  <link rel="stylesheet" href="{{ asset('bundles/matiltimensajeria/css/reset.css') }}" type="text/css" media="all">
  <link rel="stylesheet" href="{{ asset('bundles/matiltimensajeria/css/style.css') }}" type="text/css" media="all">
  <link type="text/css" rel="stylesheet" href="{{ asset('bundles/matiltimensajeria/css/jquery-ui-1.8.20.custom.css') }}">
  {% block css %}{% endblock %}
  {% block jquery %}
   <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/jquery-1.7.2.min') }}" ></script>
  {% endblock %}
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/hideshow.js') }}" ></script> <!-- Hide/Show -->
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/jquery-ui.min.js') }}" type="text/javascript"></script><!-- Hide/Show -->
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/cufon-yui.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/Humanst521_BT_400.font.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/Humanst521_Lt_BT_400.font.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/roundabout.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/roundabout_shapes.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/gallery_init.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/cufon-replace.js') }}"></script>  
  <!--[if lt IE 7]>
  	<link rel="stylesheet" href="{{ asset('bundles/matiltimensajeria/css/ie/ie6.css') }}" type="text/css" media="all">
  <![endif]-->
  <!--[if lt IE 9]>
  	<script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/html5.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/matiltimensajeria/js/IE9.js') }}"></script>
  <![endif]-->
  {% block js %}{% endblock %}
</head>

<body>
  <!-- header -->
  <header>
   <div class="container">
    <h1>{% block titleH1 %} :: Mensajeria :: {% endblock %}</h1>
    <nav>
     <ul>
      {% include 'mensajeriaBundle:Mensajeria:menu.html.twig' %}
     </ul>
    </nav>
   </div>
  </header>
   {% block galeria %}{% endblock %}
   <div class="main-box">
  {% block total %}
    <div class="container">
     <div class="inside">
      {% block contenido %}
       <div class="wrapper">
        {% block noticias %}
        {% endblock %}
        <section id="content">
         {% block articulo %}
          <article><h2>Bienvenid@ al modulo de <span>Mensajeria</span></h2></article>
         {% endblock %}
        </section>
       </div>
      {% endblock %}
      {% block aside %}
      {% endblock %}
     </div>
    </div>
  {% endblock %}
   </div>
  <!-- footer -->
  <footer>
   <div class="container">
    <div class="wrapper">
     <div class="fleft">Algunos derechos reservados <img src="{{ asset('images/etc/by-nc-sa.jpg') }}" height="35" width="100" /></div>
     <div class="fright">designed by <a rel="nofollow" href="http://www.templatemonster.com/" target="_blank">TemplateMonster.com</a> |&nbsp; &nbsp; <a href="http://templates.com/product/3d-models/" target="_blank">3D Models</a> provided by Templates.com</div>
    </div>
   </div>
  </footer>
  <script type="text/javascript"> Cufon.now(); </script>
</body>
</html>
